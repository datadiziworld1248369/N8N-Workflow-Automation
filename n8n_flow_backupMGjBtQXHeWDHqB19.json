    {
      "parameters": {
        "jsCode": "function beautifyJson(data) {\n  if (typeof data === \"string\") {\n    try {\n      return JSON.parse(data);\n    } catch (err) {\n      return `Invalid JSON string: ${err.message}`;\n    }\n  }\n\n  if (typeof data === \"object\" && data !== null) {\n    return data; // ƒë√£ l√† object\n  }\n\n  return `Invalid data type: ${typeof data}`;\n}\n\nconst input = $input.all()[0].json;\nconst stdout = input.stdout;\n\n// B·∫Øt to√†n b·ªô JSON array ho·∫∑c object t·ª´ chu·ªói stdout\nconst jsonMatch = typeof stdout === \"string\" ? stdout.match(/\\[[\\s\\S]*\\]|\\{[\\s\\S]*\\}/) : null;\n\nif (!jsonMatch) {\n  return [\n    {\n      json: {\n        error: \"No valid JSON object or array found in stdout.\"\n      }\n    }\n  ];\n}\n\n// Beautify / Parse safely\nconst parsed = beautifyJson(jsonMatch[0]);\n\n// N·∫øu l·ªói th√¨ tr·∫£ ra l·ªói\nif (typeof parsed === \"string\" && parsed.startsWith(\"Invalid\")) {\n  return [\n    {\n      json: {\n        error: parsed\n      }\n    }\n  ];\n}\n\n// N·∫øu l√† array th√¨ map t·ª´ng item ra output\nif (Array.isArray(parsed)) {\n  return parsed.map(item => {\n    if (typeof item === 'object' && item !== null) {\n      return { json: item };\n    }\n\n    if (typeof item === 'string') {\n      try {\n        const parsedItem = JSON.parse(item);\n        return { json: parsedItem };\n      } catch (err) {\n        return { json: { error: `Failed to parse item: ${err.message}` } };\n      }\n    }\n\n    return { json: { error: `Unsupported item type: ${typeof item}` } };\n  });\n}\n\n// N·∫øu l√† object th√¨ tr·∫£ v·ªÅ 1 item\nif (typeof parsed === 'object' && parsed !== null) {\n  return [{ json: parsed }];\n}\n\n// Tr∆∞·ªùng h·ª£p kh√°c\nreturn [{\n  json: {\n    error: \"Parsed result is neither array nor object.\"\n  }\n}];\n"
      },
