        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"records\": [\n    {\n      \"fields\": {\n        \"Xác_nhận\": \"Đã gửi Event thành công\"\n      },\n      \"record_id\": \"{{ $('Loop Over Items').item.json.record_id }}\"\n    }\n  ]\n}\n",
